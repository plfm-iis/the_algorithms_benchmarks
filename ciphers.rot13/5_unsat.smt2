(set-logic ALL)
(declare-const s_VAR String)
(declare-const n_VAR Int)
(assert (< 0 (str.len s_VAR)))
(assert (> (str.len s_VAR) 0))
(assert (str.in.re (str.at s_VAR 0) (re.+ (re.union (re.union (re.range "A" "Z") (re.range "a" "z")) (re.range "0" "9")))))
(assert (str.<= "A" (str.at s_VAR 0)))
(assert (not (str.in.re (str.at s_VAR 0) (re.+ (re.union (re.union (re.range "A" "Z") (re.range "a" "z")) (re.range "0" "9"))))))
(check-sat)
(get-value (s_VAR))
(get-value (n_VAR))
